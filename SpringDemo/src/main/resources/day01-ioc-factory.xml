<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--静态工厂类-->
    <bean id="userService" class="com.test.day01.ioc.factory.UserServiceFactory" factory-method="getUserService"/>

    <!--非静态工厂类-->
    <!--首先非静态工厂类实例化-->
    <bean id="empServiceFactory" class="com.test.day01.ioc.factory.EmpServiceFactory"/>
    <!--然后通过非静态工厂类的非静态方法，生成对象-->
    <bean id="empService" factory-bean="empServiceFactory" factory-method="getEmpService"/>
</beans>