<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
       xmlns:c="http://www.springframework.org/schema/c"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--p标签和c标签的产生主要是方便书写，能力上于property和constructor一致-->
    <!--使用p标签代替set注入-->
    <bean id="car" class="com.test.day02.di.bean.Car" p:brand="大众" p:price="150000"/>
    <!--<bean id="car" class="com.test.day02.di.bean.Car">
        <property name="brand" value="大众"/>
        <property name="price" value="150000"/>
    </bean>-->


    <!--使用c标签代替构造注入-->
    <bean id="car2" class="com.test.day02.di.bean.Car" c:brand="奥迪" c:price="400000"/>
    <!--<bean id="car2" class="com.test.day02.di.bean.Car">
        <constructor-arg name="brand" value="奥迪"/>
        <constructor-arg name="price" value="400000"/>
    </bean>-->
</beans>