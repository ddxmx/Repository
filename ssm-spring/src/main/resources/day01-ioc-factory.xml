<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--静态工厂类-->
    <bean id="userServiceBean" class="com.test.day01.hello.factory.UserServiceFactory" factory-method="getUserService"/>

    <!--非静态工厂类-->
    <!--首先非静态工厂类实例化-->
    <bean id="studentFactory" class="com.test.day01.hello.factory.StudentFactory"/>
    <!--然后通过非静态工厂类的非静态方法，生成对象-->
    <bean id="studentBean" factory-bean="studentFactory" factory-method="getStudent"/>
</beans>